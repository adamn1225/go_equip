# Proxy Configuration
WEBSHARE_API_KEY=your_webshare_api_key_here
WEBSHARE_PROXY_URL=https://proxy.webshare.io/api/v2/proxy/list/

# Alternative proxy providers (choose one)
# BRIGHT_DATA_USERNAME=your_bright_data_username
# BRIGHT_DATA_PASSWORD=your_bright_data_password
# BRIGHT_DATA_ENDPOINT=your_bright_data_endpoint

# Scraper Configuration  
USE_PROXY_ROTATION=true
PROXY_ROTATION_DELAY=1000  # milliseconds between proxy switches
MAX_RETRIES_PER_PROXY=3

# Browser Configuration
HEADLESS_MODE=true
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36
