# CAPTCHA Solver Configuration
# Copy this file to config.env and update with your settings

# CAPTCHA Solver Service
CAPTCHA_SOLVER_URL=http://localhost:5000
CAPTCHA_SOLVER_TIMEOUT=300s

# API Keys (optional but recommended)
CAPTCHA_2CAPTCHA_KEY=your_2captcha_api_key_here
CAPTCHA_ANTICAPTCHA_KEY=your_anticaptcha_api_key_here

# Detection Settings
CAPTCHA_DETECTION_ENABLED=true
CAPTCHA_AUTO_SOLVE_ENABLED=true
CAPTCHA_MANUAL_FALLBACK=true

# Browser Settings
CAPTCHA_BROWSER_HEADLESS=true
CAPTCHA_BROWSER_TIMEOUT=60s
